{
  "name": "swappi-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/swappi-dex/v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-local": "graph create swappi-dex/swappi --node http://127.0.0.1:8020",
    "remove-local": "graph remove swappi-dex/swappi --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy swappi-dex/swappi --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "debug": "yarn codegen && graph deploy swappi-dex/swappi --debug-fork --debug $npm_config_uuid --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "ship": "yarn codegen && yarn create-local && yarn deploy-local",
    "watch-local": "graph deploy swappi-dex/swappi --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
